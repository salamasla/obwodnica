{
  "name": "Obwodnica",
  "description": "Automatic bypass of afk check in florr.io",
  "version": "1.2",
  "manifest_version": 3,
  "icons": {
  	"16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {},
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "sandbox": {
  	"pages": ["sandbox.html"]
  },
  "host_permissions": [
    "https://florr.io/"
  ],
  "permissions": ["scripting", "activeTab", "debugger", "offscreen", "tabs"]
}